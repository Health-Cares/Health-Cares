<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
                                                   android:layout_height="match_parent">
    <TextView
            android:text="Child Care"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.634"
            app:layout_constraintBottom_toTopOf="@+id/guideline4"/>
    <androidx.constraintlayout.widget.Guideline
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_begin="109dp"
            android:id="@+id/guideline4"/>
    <com.google.android.material.button.MaterialButton
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:text="1 Month"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toTopOf="@+id/guideline4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:id="@+id/one_month"
            app:layout_constraintHorizontal_bias="0.031"/>
    <com.google.android.material.button.MaterialButton
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:text="6 Month"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintTop_toBottomOf="@+id/one_month" android:id="@+id/six_month"
            android:layout_marginTop="28dp"/>
    <com.google.android.material.button.MaterialButton
            android:layout_width="142dp"
            android:layout_height="wrap_content"
            android:text="12 Month"

            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintTop_toBottomOf="@+id/six_month" android:id="@+id/twelve_month"/>
    <com.google.android.material.button.MaterialButton
            android:layout_width="141dp"
            android:layout_height="wrap_content"
            android:text="12 >  Month"
            android:layout_marginTop="24dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.027"
            app:layout_constraintTop_toBottomOf="@+id/twelve_month" android:id="@+id/twelve_gre"/>
</androidx.constraintlayout.widget.ConstraintLayout>